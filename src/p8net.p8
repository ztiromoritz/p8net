function check_msg()
 -- check if a message is available
 -- 0x5f80..0x5fff
 local len = peek(0x5f90)
 
 if(len>0) then
  local out = ""
  for i=1,len do
    local inp = peek(0x5f80+i-1)
    out=out..chr(inp)
  end
  poke(0x5f90,0) --consumed
  return out
 end
 return nil
end

function send_msg(msg)
 local len = min(#msg,16)
 if(len>0) then
  for i=1,len do
   poke(0x5fa0+i-1,ord(msg[i]))
  end
  poke(0x5fb0,len)
 end
end

